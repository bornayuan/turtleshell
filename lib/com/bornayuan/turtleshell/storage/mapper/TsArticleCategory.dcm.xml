<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TsArticleCategory" table="ts_article_category">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="60" nullable="false"/>
    <field name="description" type="string" column="description" length="200" nullable="true"/>
    <field name="createdTimestamp" type="datetime" column="created_timestamp" nullable="false"/>
    <field name="updatedTimestamp" type="datetime" column="updated_timestamp" nullable="true"/>
  </entity>
</doctrine-mapping>
